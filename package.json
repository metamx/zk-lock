{
  "name": "zk-lock",
  "version": "1.0.1",
  "description": "A distributed lock using zookeeper",
  "keywords": [
    "zookeeper",
    "zookeeper lock",
    "distributed lock"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "http://github.com/metamx/zk-beacon.git"
  },
  "main": "build/index.js",
  "typescript": {
    "definition": "build/zk-lock.d.ts"
  },
  "scripts": {
    "typings": "node_modules/.bin/typings install",
    "compile": "node_modules/.bin/grunt",
    "build": "npm run typings && npm run compile",
    "test": "node_modules/.bin/mocha",
    "debugtest": "NODE_DEBUG=zk-lock ./node_modules/.bin/mocha",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "bluebird": "^3.4.2",
    "locators": "^1.0.0",
    "node-zookeeper-client": "^0.2.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coffee-script": "^1.10.0",
    "dts-generator": "^1.7.0",
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-ts": "^5.5.1",
    "grunt-tslint": "^3.2.1",
    "mocha": "^3.0.2",
    "nock": "^8.0.0",
    "tslint": "^3.15.1",
    "typescript": "^1.8.10",
    "typings": "^1.3.2"
